# Keeviqo Deployment Environment Variables
# Copy this file to .env.deploy and fill in your values

# Application settings
APP_NAME=keeviqo
DEPLOY_DIR=/var/www/keeviqo
REPO_URL=https://github.com/adi877-hub/Keeviqo-v1.git
BRANCH=main

# Database settings
DB_NAME=keeviqo_prod
DB_USER=keeviqo_user
DB_PASSWORD=your_secure_password_here

# Domain settings
DOMAIN=yourdomain.com

# Security settings
SESSION_SECRET=your_session_secret_here
